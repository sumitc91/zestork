	<script src="../../Resource/templates/afterLogin/js/plugins/validation/jquery.validate.min.js"></script>
	<script src="../../Resource/templates/afterLogin/js/plugins/validation/additional-methods.min.js"></script>
	<!-- icheck -->
	<script src="../../Resource/templates/afterLogin/js/plugins/icheck/jquery.icheck.min.js"></script>
