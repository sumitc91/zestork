	<link rel="stylesheet" href="../../Resource/templates/afterLogin/css/themes.css">


	<!-- Nice Scroll -->
	<script src="../../Resource/templates/afterLogin/js/plugins/nicescroll/jquery.nicescroll.min.js"></script>
