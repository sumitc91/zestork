	<script src="../../Resource/templates/afterLogin/js/bootstrap.min.js"></script>
	<script src="../../Resource/templates/afterLogin/js/eakroko.js"></script>

	<!--[if lte IE 9]>
		<script src="js/plugins/placeholder/jquery.placeholder.min.js"></script>
