<script type="text/javascript" src="../../Resource/templates/beforeLogin/web/js/responsiveslides.min.js"></script>
		  
        <script src="../../Content/lib/angular.min.js" type="text/javascript"></script>  
		<script src="../../Content/app/beforeLogin/beforeLoginApp.js" type="text/javascript"></script>				
		<script src="../../Content/app/beforeLogin/homeTemplate.js" type="text/javascript"></script>
		<script src="../../Content/app/beforeLogin/serviceTemplate.js" type="text/javascript"></script>
		<script src="../../Content/app/beforeLogin/aboutTemplate.js" type="text/javascript"></script>

		<link type="text/css" rel="stylesheet" href="../../Content/third-party/jquery.modalbox-1.5.0/css/jquery.modalbox.css" />
		<script type="text/javascript" src="http//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
		<script type="text/javascript" src="../../Content/third-party/jquery.modalbox-1.5.0/js/jquery.modalbox-1.5.0-min.js"></script>
		<!--http://code.google.com/p/jquery-modalbox-plugin/-->

		<!-- Bootstrap -->
	<link rel="stylesheet" href="../../Resource/templates/afterLogin/css/bootstrap.min.css">
